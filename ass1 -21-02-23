//program to print given number is palindrome or not
#include<stdio.h>
#include<conio.h>
void main()
{
int n,r,sum=0,temp;
clrscr();
printf("\n enter the number");
scanf("\n%d",&n);
temp=n;
while(n!=0)
{
r=n%10;
sum=sum*10+r;
n=n/10;
}
if(temp==sum)
printf("\n given number %d is a palindrome");
else
printf("\n given number%d is not a palindrome");
getch();
}
